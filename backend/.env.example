# Copy this file to .env and fill in your actual values

# Django Settings
DJANGO_SECRET_KEY=change-me-to-a-secure-secret-key-in-production
DJANGO_DEBUG=True
DJANGO_ALLOWED_HOSTS=127.0.0.1,localhost

# WorkOS / AuthKit - Get these from your WorkOS dashboard
WORKOS_API_KEY=sk_your_api_key_here
WORKOS_CLIENT_ID=client_your_client_id_here
WORKOS_REDIRECT_URI=http://localhost:3000/auth/callback
WORKOS_COOKIE_PASSWORD=32-character-minimum-secret-key-replace-this

# App JWT
SIMPLEJWT_SIGNING_KEY=change-me-to-a-secure-jwt-signing-key

# CORS
CORS_ALLOWED_ORIGINS=http://localhost:3000